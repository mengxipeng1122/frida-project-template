{
  "name": "frida-project-template-agent",
  "version": "1.0.0",
  "description": "Frida agent written in TypeScript",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "build:server": "tsc -p tsconfig.json",
    "build:client": "webpack --mode production",
    "build": "npm run build:client && npm run build:server",
    "start:server": "node dist/server/server.js",
    "start:client": "webpack serve --mode development --open",
    "dev:server": "nodemon --watch src/server --exec 'npm run build:server && npm run start:server'",
    "dev:client": "webpack serve --mode development --open",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "devDependencies": {
    "@types/frida-gum": "^18.3.1",
    "@types/node": "^18.14.0",
    "frida-compile": "^16.1.8",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "concurrently": "^9.0.1",
    "express": "^4.21.0",
    "nodemon": "^3.1.7"
  }
}
