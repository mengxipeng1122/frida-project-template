{
  "name": "frida-project-template-agent",
  "version": "1.0.0",
  "description": "Frida agent written in TypeScript",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "concurrently \"ts-node src/server.ts\" \"nodemon --watch src/client.ts --watch src/public/ -e ts,js,html,css --exec 'npm run build'\"",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@types/frida-gum": "^18.3.1",
    "@types/node": "^18.14.0",
    "frida-compile": "^16.1.8",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "concurrently": "^9.0.1",
    "express": "^4.21.0",
    "nodemon": "^3.1.7",
    "ts-frida": "^1.0.23",
    "ts-node": "^10.9.2"
  }
}
